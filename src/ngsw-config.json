{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name": "PWA2",
      "urls":[
        "https://pwaapp-1f5db.firebaseio.com"
      ],
      "cacheConfig":{
        "strategy": "performance",
        "maxAge": "1d",
        "maxSize": "100"
      }
    }
  ]
}
